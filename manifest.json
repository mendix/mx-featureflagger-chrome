{
	"name": "Mx Studio Feature Flagger",
	"version": "1.0",
	"description": "Makes using feature flags in Mendix Studio so easy your dog could do it!",
	"permissions": ["activeTab", "storage"],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/mx-beta16.png",
			"32": "images/mx-beta32.png",
			"48": "images/mx-beta48.png",
			"128": "images/mx-beta128.png"
		}
	},
	"icons": {
		"16": "images/mx-beta16.png",
		"32": "images/mx-beta32.png",
		"48": "images/mx-beta48.png",
		"128": "images/mx-beta128.png"
	},
	"manifest_version": 2
}
